# AEE
ro.vendor.have_aee_feature=1

# Audio
persist.sys.audio.out.vol=7
ro.audio.silent=0
ro.audio.usb.period_us=16000
ro.camera.sound.forced=0
ro.config.hd_voicemail=1
ro.config.vc_call_vol_steps=10
ro.mtk_audenh_support=1
ro.mtk_audio_lossless_support=1
ro.mtk_besloudness_support=1

# Brand
Build.BRAND=MTK

# Charger
ro.charger.enable_suspend=true

# Display
debug.sf.disable_backpressure=1
debug.sf.enable_gl_backpressure=0
ro.opengles.version=196610
ro.sf.blurs_are_expensive=1
ro.surface_flinger.supports_background_blur=1

# Perf
ro.mtk_perf_fast_start_win=1
ro.mtk_perf_response_time=1
ro.mtk_perf_simple_start_win=1

# Media
media.stagefright.thumbnail.prefer_hw_codecs=true
ro.vendor.mtk_flv_playback_support=1
vendor.mtk_thumbnail_optimization=true

# Camera
ro.mtk_cam.mfb.ultimate=1
persist.camera.HAL3.enabled=1
persist.camera.eis.enable=1
persist.vendor.camera.privapp.list=com.android.camera,com.google.camera,org.lineageos.aperture,com.oplus.camera
vendor.camera.aux.packageexcludelist=org.telegram.messenger,com.microsoft.teams,com.discord,us.zoom.videomeetings
vendor.camera.aux.packagelist=com.android.camera,com.google.camera,com.oplus.camera,org.lineageos.aperture

# Radio
persist.sys.radio.force_lte_ca=true

# IMS
persist.dbg.ims_volte_enable=1
persist.dbg.volte_avail_ovr=1
persist.dbg.vt_avail_ovr=1
persist.dbg.wfc_avail_ovr=1
persist.mtk.volte.enable=1
ro.mtk_ims_support=1
ro.mtk_volte_support=1
ro.vendor.md_auto_setup_ims=1

# LMKD
persist.sys.lmk.reportkills=false
ro.config.low_ram=false
ro.lmk.log_stats=true
sys.lmk.reportkills=0

# USB
ro.sys.usb.bicr=no
ro.sys.usb.charging.only=yes
ro.sys.usb.mtp.whql.enable=0
ro.sys.usb.storage.type=mtp

# Storage
persist.sys.fuse.passthrough.enable=true

# IPO
sys.ipo.disable=1
sys.ipo.pwrdncap=2

# iorapd
ro.iorapd.enable=false

# Kernel
ro.kernel.zio=38,108,105,16

# Memory
ro.config.per_app_memcg=false

# Misc
persist.vendor.mdlog.flush_log_ratio=72340194296137244
persist.vendor.pms_removable=1
persist.vendor.vzw_device_type=0
qemu.hw.mainkeys=0
ro.mediatek.version.branch=alps-mp-s0.mssi1.tc16sp-pr6
ro.support_one_handed_mode=true
ro.vendor.mtk_cta_set=1
ro.vendor.mtk_gwsd_support=1
ro.vendor.mtk_omacp_support=1

# WiFi
mediatek.wlan.ctia=0
persist.data.iwlan=1
persist.data.iwlan.enable=true
persist.data.iwlan.ipsec.ap=1
persist.vendor.wfc.sys_wfc_support=1
ro.mediatek.wlan.p2p=1
ro.mediatek.wlan.wsc=1
ro.vendor.net.enable_dual_wifi=1
wifi.direct.interface=p2p0
wifi.interface=wlan0
wifi.tethering.interface=ap0

# RIL
ro.telephony.sim.count=2
ro.vendor.mtk_telephony_add_on_policy=0
vendor.rild.libargs=-d /dev/ttyC0
vendor.rild.libpath=/vendor/lib64/libmtk-ril.so

# Zygote
persist.device_config.runtime_native.usap_pool_enabled=true
ro.zygote.preload.enable=0
zygote.critical_window.minute=10

# Memory management
persist.sys.fw.bg_apps_limit?=48
persist.sys.fw.bservice_age?=120000
persist.sys.fw.bservice_enable?=true
persist.sys.fw.bservice_limit?=6
persist.sys.fw.empty_app_percent?=50
persist.sys.fw.trim_cache_percent?=100
persist.sys.fw.trim_empty_percent?=100
persist.sys.fw.trim_enable_memory?=2147483648
persist.sys.fw.use_trim_settings?=true
ro.config.hw_ram=true

# GPU Performance
debug.composition.type=gpu
debug.egl.hw=1
debug.hwui.render_dirty_regions=false
debug.hwui.renderer=skiagl
debug.sf.hw=1
ro.mtk_gpu_optimizations=1

# Network Optimization
persist.radio.apm_sim_not_pwdn=1
persist.radio.smart_ap=1
ro.mtk_data_speed_label=true
ro.mtk_network_management=1

# Battery Optimization
pm.sleep_mode=1
ro.config.max.starting_bg=4
ro.sys.fw.bg_apps_limit=4
